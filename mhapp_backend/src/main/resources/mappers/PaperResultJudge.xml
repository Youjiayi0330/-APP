<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mappers.PaperResultJudgeMapper">
    <select id="getPaperJudge" resultType="com.example.demo.entity.PaperResultJudge">
        select * from paper_result_judge
        where paper_id = #{paperId}
